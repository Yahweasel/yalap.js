{
  "name": "yalap.js",
  "//version": "When updating the version, make sure you update it here (in every place it appears), in the Makefile, and in the test(s).",
  "version": "1.0.0",
  "description": "Yet another libarchive port to JavaScript/WebAssembly. Aims to have feature-reduced builds that aren't bloated by tasks you don't need.",
  "main": "dist/yalap-1.0.0-all.js",
  "types": "yalap.d.ts",
  "exports": {
      ".": "./dist/yalap-1.0.0-all.js",
      "./archive": "./dist/yalap-1.0.0-archive.js",
      "./extract": "./dist/yalap-1.0.0-extract.js",
      "./all": "./dist/yalap-1.0.0-all.js",
      "./zip": "./dist/yalap-1.0.0-zip.js",
      "./unzip": "./dist/yalap-1.0.0-unzip.js",
      "./arcex-zip": "./dist/yalap-1.0.0-arcex-zip.js",
      "./7zip": "./dist/yalap-1.0.0-7zip.js",
      "./un7zip": "./dist/yalap-1.0.0-un7zip.js",
      "./arcex-7zip": "./dist/yalap-1.0.0-arcex-7zip.js",
      "./tar": "./dist/yalap-1.0.0-tar.js",
      "./untar": "./dist/yalap-1.0.0-untar.js",
      "./arcex-tar": "./dist/yalap-1.0.0-arcex-tar.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yahweasel/yalap.git"
  },
  "keywords": [
    "libarchive",
    "archive",
    "zip",
    "extract",
    "compress",
    "decompress"
  ],
  "author": "Yahweasel",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/Yahweasel/yalap/issues"
  },
  "homepage": "https://github.com/Yahweasel/yalap#readme",
  "devDependencies": {
    "typescript": "^5.3.3",
    "uglify-js": "^3.17.4"
  }
}
